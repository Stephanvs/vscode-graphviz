{
    "name": "dot",
    "displayName": "Graphviz (dot) language support for Visual Studio Code",
    "description": "This extension provides GraphViz (dot) language support for Visual Studio Code",
    "version": "0.0.1",
    "publisher": "Stephanvs",
    "engines": {
        "vscode": "^0.10.1"
    },
    "icon": "img/GraphvizLogo.png",
    "galleryBanner": {
        "color": "#69c",
        "theme": "light"
    },
    "categories": [
        "Languages",
        "Snippets"
    ],
    "contributes": {
        "languages": [
            {
                "id": "dot",
                "aliases": [
                    "Graphviz (DOT)",
                    "dot"
                ],
                "extensions": [
                    ".dot",
                    ".DOT",
                    ".gv"
                ],
                "configuration": "./dot.configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "dot",
                "scopeName": "source.dot",
                "path": "./syntaxes/dot.tmLanguage"
            }
        ],
        "snippets": [
            {
                "language": "dot",
                "path": "./snippets/snippets/dot.json"
            }
        ]
    },
    "license": "https://github.com/Stephanvs/vscode-graphviz/blob/master/LICENSE",
    "bugs": {
        "url": "https://github.com/Stephanvs/vscode-graphviz/issues"
    },
    "homepage": "https://github.com/Stephanvs/vscode-graphviz/blob/master/README.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/Stephanvs/vscode-graphviz.git"
    }
}